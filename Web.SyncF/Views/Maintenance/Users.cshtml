@section cardheader{
    <h4>Users</h4>
}
<div id="container">

</div>


<script>
    $(document).ready(function() {
        load();
    });

    function load() {
        $.ajax({
            url: "@Url.Action("UsersGridPartial")",
            success: function(data) {
                $('#container').html(data);
            }
        })
    }
</script>